<AnchorPane fx:id="anchorPane" prefHeight="600.0" prefWidth="1067.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ControllerCAMainDashboard">
    <children>
        <AnchorPane prefHeight="65.0" prefWidth="1066.0">
            <children>
                <JFXHamburger fx:id="hamburger" layoutX="14.0" layoutY="22.0" />
                <Text layoutX="301.0" layoutY="51.0" strokeType="OUTSIDE" strokeWidth="0.0"
                      text="FireE - Network Capture" wrappingWidth="465.13671875">
                    <font>
                        <Font name="Marlett" size="40.0" />
                    </font>
                </Text>
                <Line endX="1067.0" endY="65.0" startY="65.0" strokeWidth="1.8" />
            </children>
        </AnchorPane>
        <JFXToggleButton fx:id="captureToggle" layoutX="372.0" layoutY="71.0" text="Capture Network Packets">
            <font>
                <Font name="Lucida Sans Regular" size="20.0" />
            </font>
        </JFXToggleButton>
        <JFXButton fx:id="clearCaptureBtn" layoutX="750.0" layoutY="84.0" onAction="#ClearPackets" text="Clear Current Capture" textFill="RED">
            <font>
                <Font name="Lucida Sans Regular" size="20.0" />
            </font>
        </JFXButton>
        <ScrollPane layoutY="128.0" prefHeight="473.0" prefWidth="1069.0">
            <content>
                <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
                    <children>
                        <LineChart fx:id="networkTrafficChart" layoutX="-1.0" layoutY="1.0" prefHeight="334.0" prefWidth="1051.0" title="Current Network Traffic">
                            <xAxis>
                                <CategoryAxis side="BOTTOM" />
                            </xAxis>
                            <yAxis>
                                <NumberAxis side="LEFT" />
                            </yAxis>
                        </LineChart>
                        <PieChart fx:id="protocolChart" layoutY="325.0" />
                        <PieChart fx:id="top10IPChart" layoutX="538.0" layoutY="325.0" />
                    </children></AnchorPane>
            </content>
        </ScrollPane>
        <Label fx:id="alertCount" layoutX="11.0" layoutY="93.0" prefHeight="26.0" prefWidth="353.0"
               text="Suspicious Events Count: &lt;Number&gt;">
            <font>
                <Font name="Lucida Sans Regular" size="18.0"/>
            </font>
        </Label>
        <JFXDrawer fx:id="drawer" layoutY="66.0" prefHeight="535.0" prefWidth="219.0" />
    </children>
