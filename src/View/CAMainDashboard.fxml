                <Font name="Lucida Sans Regular" size="20.0" />
            </font>
        </JFXButton>
        <ScrollPane layoutY="128.0" prefHeight="473.0" prefWidth="1069.0">
            <content>
                <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
                    <children>
                        <PieChart fx:id="protocolChart" layoutY="342.0" title="Protocol Make-Up"/>
                        <PieChart fx:id="top10IPChart" layoutX="538.0" layoutY="342.0" title="Top 5 IP Addresses"/>
                        <AnchorPane fx:id="LineChartAnchorPane" layoutX="-1.0" layoutY="1.0" maxHeight="-Infinity"
                                    maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="334.0"
                                    prefWidth="1051.0"/>
                    </children></AnchorPane>
            </content>
        </ScrollPane>
        <Label fx:id="alertCount" layoutX="11.0" layoutY="93.0" prefHeight="26.0" prefWidth="353.0"
