                   wrappingWidth="465.13671875">
               <font>
                  <Font name="Marlett" size="40.0" />
               </font>
            </Text>
            <Line endX="1067.0" endY="65.0" startY="65.0" strokeWidth="1.8" />
         </children>
      </AnchorPane>
      <StackPane layoutY="139.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="461.0" prefWidth="1069.0">
         <children>
            <TableView fx:id="packetstable" onMouseClicked="#ShowpacketDetails">
               <columns>
                  <TableColumn fx:id="tableColPN" editable="false" prefWidth="96.0" text="Packet Number" />
                  <TableColumn fx:id="tableColSrcIP" editable="false" prefWidth="138.0" text="Source IP Address" />
                  <TableColumn fx:id="tableColSrcPort" editable="false" prefWidth="85.0" text="Source Port" />
